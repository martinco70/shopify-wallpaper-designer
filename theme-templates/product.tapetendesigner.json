        "custom_liquid_XEmXbe": {
          "type": "custom_liquid",
          "name": "Produktinformationen (Custom Liquid)",
          "settings": {
            "custom_liquid": "<h3>Produktinformationen:</h3>\n{% if product.metafields.custom.bahnenbreite_in_cm or\n      product.metafields.custom.rollenlaenge or\n      product.metafields.custom.rapport or\n      product.metafields.custom.warenbreite or\n      product.metafields.custom.tapeteneigenschaften or\n      product.metafields.custom.kollektion2 or\n      product.metafields.custom.designname %}\n\n  <ul>\n    {% if product.metafields.custom.bahnenbreite_in_cm %}\n      <li><h5>Bahnenbreite: {{ product.metafields.custom.bahnenbreite_in_cm }} cm</h5></li>\n    {% endif %}\n    {% if product.metafields.custom.rollenlaenge %}\n      <li><h5>Rollenlänge: {{ product.metafields.custom.rollenlaenge }} cm</h5></li>\n    {% endif %}\n    {% if product.metafields.custom.rapport %}\n      <li><h5>Rapport: {{ product.metafields.custom.rapport }} cm</h5></li>\n    {% endif %}\n    {% if product.metafields.custom.warenbreite %}\n      <li><h5>Breite des Bildes: {{ product.metafields.custom.warenbreite }} cm</h5></li>\n    {% endif %}\n    {% if product.metafields.custom.kollektion2 %}\n  {% assign kollektion_clean = product.metafields.custom.kollektion2.value | replace: '\"', '' | replace: '[', '' | replace: ']', '' %}\n  <li><h5>Kollektion: {{ kollektion_clean | strip }}</h5></li>\n{% endif %}\n    \n  </ul>\n{% endif %}"
          }
        },
        "custom_liquid_Q9efFc": {
          "type": "custom_liquid",
          "name": "Tapeteneigenschaften (Custom Liquid)",
          "settings": {
            "custom_liquid": "{% if product.metafields.custom.tapeteneigenschaften %}\n   <h3>Tapeteneigenschaften:</h3>\n<ul>\n      {% assign tapeteneigenschaften_list = product.metafields.custom.tapeteneigenschaften.value | split: ',' %}\n      {% for eigenschaft in tapeteneigenschaften_list %}\n    {% assign eigen_clean = eigenschaft | replace: '\"', '' | replace: '[', '' | replace: ']', '' %}\n\n        <li><h5>{{ eigen_clean | strip }}</h5></li>\n      {% endfor %}\n    {% endif %}"
          }
        },
        "wpd_material_markers": {
          "type": "custom_liquid",
          "name": "WPD Material Markers",
          "settings": {
            "custom_liquid": "{% if product.metafields.custom.material %}<!-- WPD Material Markers START -->\n<meta name=\"wpd:material\" content=\"{{ product.metafields.custom.material | escape }}\">\n<div id=\"wpd-material-source\" data-product-material=\"{{ product.metafields.custom.material | escape }}\" style=\"display:none\"></div>\n<script>(function(){window.__WPD_MATERIAL__={material:'{{ product.metafields.custom.material | escape }}'};})();</script>\n<!-- WPD Material Markers END -->{% endif %}"
          }
        },
